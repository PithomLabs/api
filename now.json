{
	"version": 2,
	"name": "api",
	"scope": "komfy",
	"builds": [
			{
				"src": "/lambdas/index.go",
				"use": "@now/go"
			}, 
			{
				"src": "/lambdas/rand/rand.go",
				"use": "@now/go"
			},
			{
				"src": "/lambdas/randDict/randDict.go",
				"use": "@now/go",
				"config": {
					"includeFiles": [
						"template/**"
					]
				}
			}
	], "routes": [
		{"src": "/", "dest": "/lambdas/index.go"},
		{"src": "/rand", "dest": "/lambdas/rand/rand.go"},
		{"src": "/randDict", "dest": "/lambdas/randDict/randDict.go"}
	],
	"build": {
		"env": {
		  "GO111MODULE": "auto"
		}
	}
}
