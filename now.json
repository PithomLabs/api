{
	"version": 2,
	"name": "api",
	"scope": "komfy",
	"builds": [
		{
			"src": "/lambdas/**/*.go",
			"use": "@now/go"
		}
	], "routes": [
		{"src": "/", "dest": "/lambdas/index/index.go"},
		{"src": "/rand", "dest": "/lambdas/rand/rand.go"},
		{"src": "/rand_dict", "dest": "/lambdas/randdict/randdict.go"}
	],
	"build": {
		"env": {
		  "GO111MODULE": "on"
		}
	}
}
