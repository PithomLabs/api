{
	"version": 2,
	"name": "api",
	"scope": "komfy",
	"builds": [
		{
			"src": "handler.go",
			"use": "@now/go"
		}
	], "routes": [
		{
			"src": "/", 
			"dest": "handler.go"
		},
		{
			"src": "/(.*)", 
			"dest": "handler.go"
		}
	],
	"build": {
		"env": {
		  "GO111MODULE": "auto"
		}
	}
}
