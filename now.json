{
	"version": 2,
	"name": "api",
	"scope": "komfy",
	"builds": [
		{
			"src": "/index/index.go",
			"use": "@now/go"
		},
		{
			"src": "/rand/rand.go",
			"use": "@now/go"
		},
		{
			"src": "/randDict/randDict.go",
			"use": "@now/go"
		}
	], "routes": [
		{"src": "/", "dest": "/index/index.go"},
		{"src": "/rand", "dest": "/rand/rand.go"},
		{"src": "/randDict", "dest": "/randDict/randDict.go"}
	],
	"build": {
		"env": {
		  "GO111MODULE": "on"
		}
	}
}
